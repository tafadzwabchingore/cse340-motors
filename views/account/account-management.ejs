<!-- Content Heading -->
<% if (title) { %>
    <h1><%= title %></h1>
    <% } else {
      res.redirect('/')
} %>

<!-- Message Block -->
<%- messages()  %>
<% if (errors) { %>
  <ul class="notice">
<% errors.array().forEach(error => { %>
  <li><%= error.msg %></li>
<%  }) %>
  </ul>
<% } %>

<!-- Body -->
<div class="accountManagement">
  <h2>Welcome  <%=locals.accountData.account_firstname %></h2>
  <p>You're logged in!</p>
</div>